{
  "name": "batch_prof",
  "process.type": "batch",
  "data.sources": [
    {
      "name": "src",
      "baseline": true,
      "connectors": [
        {
          "type": "jdbc",
          "version": "1.0",
          "config": {
            "database": "dp_stag",
            "table.name": "aPK_table",
            "jdbc.url": "jdbc:mysql://59.111.61.142:3306/",
            "jdbc.user": "root",
            "jdbc.password": "datapipeline123"
          }
        }
      ]
    }
  ],
  "evaluate.rule": {
    "rules": [
      {
        "dsl.type": "griffin-dsl",
        "dq.type": "profiling",
